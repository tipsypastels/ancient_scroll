<% cache @objects do %>
  <div class="title-bar visible-links">
    <h1 class="title-bar__heading">
      <%= @title %>
    </h1>

    <nav class="title-bar__nav">
      <%= link_to @new_object_path, class: 'button' do %>
        <%= fa :plus %>
        New
      <% end %>
    </nav>
  </div>

  <div class="object-category-top4">
    <%= render partial: 'top4', collection: @top4, as: :object %>
  </div>

  <div class="object-category-list">
    <%= render partial: 'list_item', collection: @list, as: :object %>
  </div>
<% end %>